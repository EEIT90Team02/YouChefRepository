<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	
	
	<bean id="captcha.success" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/member/registerTest.jsp"/>
	</bean>
	<bean id="captcha.fail"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/member/registerTest.jsp"/>
	</bean>
	<bean id="register.error"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/member/register.jsp"/>
	</bean>
	<bean id="register.success"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index.jsp"/>
	</bean>
	<bean id="login.fail"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/member/login.jsp"/>
	</bean>
	<bean id="login.success"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index.jsp"/>
	</bean>
	<bean id="email"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/email/mailbox.jsp"/>
	</bean>
	<bean id="email.delete"	class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/email/mailbox.jsp"/>
	</bean>
	
</beans>
